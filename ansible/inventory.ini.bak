[deployment]
server-a ansible_host='{{ jump_host }}' ansible_user='{{ ssh_user }}'ansible_ssh_private_key_file=../terraform/resources/external.pem

[deployment:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ProxyCommand="ssh -W %h:%p -q {{ ansible_host }}"'

